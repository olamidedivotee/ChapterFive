package ChapterFive;

import java.util.Scanner;

public class Exercise11 {
    public static void main(String[] args) {
        int counter = 0;
        Scanner scanner = new Scanner(System.in);

        System.out.print("Enter a value: ");

        while (scanner.hasNext()) {
            int grade = scanner.nextInt();
            ++counter;
        }
    }
}
